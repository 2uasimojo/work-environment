alias ocm-production="OCM_CONFIG=$HOME/.config/ocm/production/config ocm"
alias ocm-staging="OCM_CONFIG=$HOME/.config/ocm/staging/config ocm"
alias ocm-integration="OCM_CONFIG=$HOME/.config/ocm/integration/config ocm"

alias ocm-prod=ocm-production
alias ocm-stg=ocm-staging
alias ocm-int=ocm-integration
